{
  "name": "sample-abap",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "transpile": "rm -rf output && abap_transpile",
    "unit": "npm run transpile && echo RUNNING && node output/index.mjs && echo OK",
    "lint": "abaplint",
    "test": "npm run lint && npm run unit",
    "run": "node run.mjs",
    "bundle": "esbuild run.mjs --bundle --outdir=dist/esm --format=esm --target=es2022 --external:tls --external:net --external:util --external:util/types --external:crypto --external:zlib --external:http --external:https --external:fs --external:path --external:url --inject:./polyfills/buffer.js --inject:./polyfills/process.js --out-extension:.js=.mjs"
  },
  "dependencies": {
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@abaplint/cli": "^2.102.42",
    "@abaplint/runtime": "^2.7.97",
    "@abaplint/transpiler-cli": "^2.7.97",
    "dotenv": "^16.3.1"
  },
  "author": "",
  "license": "EPL"
}